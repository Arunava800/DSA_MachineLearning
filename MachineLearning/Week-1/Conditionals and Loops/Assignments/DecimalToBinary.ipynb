    "n = int(input())\n",
    "binary = 0\n",
    "i = 0\n",
    "while n > 0:\n",
    "    remainder = n % 2\n",
    "    binary = remainder * (10**i) + binary\n",
    "    n = n//2\n",
    "    i += 1\n",
    "print(binary)"
